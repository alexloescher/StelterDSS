@model Ingeniux.Runtime.ICMSElement
@using Ingeniux.Runtime
@using System.Xml.Linq;
@{
	var visiblexs = "visible-xs";
        if(_Functions.isMobile()){
            if (Request.QueryString["fullView"] == "true")
            {
		visiblexs = "";
            } else if (Request.QueryString["fullView"] == "false")
		{
		//nothing
		} else {
		if(Session["desktopView"] == "true"){
		visiblexs="";
		} else if (Session["desktopView"]=="false"){
		//nothing
		}else{
		//nothing
		}
		}
        }
    }
<nav role="navigation" class="navbar navbar-default hidden-print hidden-lg nav-small stl-nav-sm visible-sm @visiblexs">
    <div class="logo" style="text-align: center;">
        <a href="@_Functions.GetLinkActionNew(Model.GetLinkItem("OrgHomeLink"),Url)" style="margin: 0 auto;">
            @Html.Partial("Editable/ImageField", Model.Element("OrgLogo"))
        </a>
    </div>
    <div class="navbar-header hidden-sm">
        <button data-target="#bs-example-navbar-collapse-1" data-toggle="collapse" class="navbar-toggle" type="button">
		    <span class="sr-only">Toggle navigation</span>
		    <span class="icon-bar"></span>
		    <span class="icon-bar"></span>
		    <span class="icon-bar"></span>
	    </button>
	    <a href="#" class="navbar-brand">Quick Links</a>
    </div>
    <div id="bs-example-navbar-collapse-1" class="collapse navbar-collapse">
        <ul class="nav navbar-nav">
            @foreach (ICMSLinkElement qLink in Model.Elements("Page", "Mainheaderlinks"))
            {
                <li>
                    <a href="@_Functions.GetLinkActionNew(qLink, Url)" target="@qLink.TargetID">@qLink.LinkName</a>
                </li>
            }
        </ul>
    </div>
</nav>