@model Ingeniux.Runtime.CMSPageRequest
@using Ingeniux.Runtime
@{
    var page = Model;
    if (page.IsPreview)
	{
        <link rel="stylesheet" href="//cdn.jsdelivr.net/jquery.magnific-popup/0.9.9/magnific-popup.css"/>        
    } 
    else
    {
        <link rel="stylesheet" href="/CSS/magnific-popup.css"/>
    } 
} 